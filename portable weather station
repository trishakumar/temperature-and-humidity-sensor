#include <DHT.h>
#define Type DHT11
#include <LiquidCrystal.h>
int rs = 7;
int en = 8;
int d4 = 9;
int d5 = 10;
int d6 = 11;
int d7 = 12;
LiquidCrystal lcd(rs,en,d4,d5,d6,d7);
int sense = 2;
DHT HT(sense, Type);
float humidity;
float tempC;
float tempF;
int setTime = 500;
void setup() {
  // put your setup code here, to run once:
Serial.begin(9600);
HT.begin();
delay(setTime);
lcd.begin(16,2);

}

void loop() {
  // put your main code here, to run repeatedly:
humidity = HT.readHumidity();
tempC = HT.readTemperature();
tempF = HT.readTemperature(true);
lcd.setCursor(0,0);
lcd.print("H = ");
lcd.setCursor(0,1);
lcd.print(humidity);
delay(1000);
lcd.clear();
lcd.setCursor(0,0);
lcd.print("TC = ");
lcd.setCursor(0,1);
lcd.print(tempC);
delay(1000);
lcd.clear();
lcd.print("TF = ");
lcd.setCursor(0,1);    
lcd.print(tempF);
delay(1000);
lcd.clear();

Serial.print("Humidity: ");
Serial.print(humidity);
Serial.print("Temperature c ");
Serial.print(tempC);
Serial.print(" C ");
Serial.print(tempF);
Serial.println(" F ");
delay(1000);
}
